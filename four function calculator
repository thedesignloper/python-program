def sum(a,b):
    print(a+b)
def diff(a,b):
    print(a-b)
def mul(a,b):
    print(a*b)
def div(a,b):
    print(a/b)
t=1
while(t>0):
    print("enter first no.")
    a=int(input())
    print("enter second no.")
    b=int(input())
    print("enter  the operation")
    d=input()
    if(d=="+"):
        sum(a,b)
    elif(d=="-"):
        diff(a,b)
    elif(d=="*"):
        mul(a,b)
    elif(d=="/"):
        div(a,b)
    else:
        t=0
